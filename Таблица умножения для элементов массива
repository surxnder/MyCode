using System;

namespace HelloWorld
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] sum = {2, 3, 7, 8, 10};

            for (int i = 0; i < 10; i++)
            {
                for (int a = 2; a < 10; a++)
                {
                    Console.WriteLine($"Умножение на:{a} {sum[i] * a}");
                }
                Console.WriteLine();
            }

        }
    }
}
